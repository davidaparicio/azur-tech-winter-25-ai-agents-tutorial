# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=langfuse

# ClickHouse Configuration
CLICKHOUSE_USER=clickhouse
CLICKHOUSE_PASSWORD=your_clickhouse_password_here
CLICKHOUSE_DB=langfuse

# Redis Configuration
REDIS_AUTH=your_redis_password_here

# S3/MinIO Configuration
LANGFUSE_S3_EVENT_UPLOAD_BUCKET=langfuse
LANGFUSE_S3_EVENT_UPLOAD_REGION=auto
LANGFUSE_S3_EVENT_UPLOAD_ACCESS_KEY_ID=minioadmin
LANGFUSE_S3_EVENT_UPLOAD_SECRET_ACCESS_KEY=your_minio_password_here
LANGFUSE_S3_EVENT_UPLOAD_ENDPOINT=http://minio:9000

# Langfuse Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
SALT=your_salt_here
NEXTAUTH_URL=http://localhost:3003
ENCRYPTION_KEY=generate_with_openssl_rand_hex_32

# Database Connection URLs (auto-generated from above variables)
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@db:5432/${POSTGRES_DB}
CLICKHOUSE_URL=http://${CLICKHOUSE_USER}:${CLICKHOUSE_PASSWORD}@clickhouse:8123/${CLICKHOUSE_DB}
CLICKHOUSE_MIGRATION_URL=clickhouse://${CLICKHOUSE_USER}:${CLICKHOUSE_PASSWORD}@clickhouse:9000/${CLICKHOUSE_DB}
